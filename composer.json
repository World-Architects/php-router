{
    "name": "crysalead/router",
    "description": "HTTP Request Router",
    "keywords": ["Router"],
    "license": "MIT",
    "authors": [
        {
            "name": "CrysaLEAD"
        }
    ],
    "require": {
        "php": ">=7",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "kahlan/kahlan": "~3.0",
        "crysalead/net": "dev-master",
        "squizlabs/php_codesniffer": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Lead\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lead\\Router\\Spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}
