{
    "name": "crysalead/router",
    "description": "HTTP Request Router",
    "keywords": ["Router"],
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "crysalead/collection": "~2.0",
        "crysalead/net": "dev-master",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "crysalead/kahlan": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Lead\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lead\\Router\\Spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}
